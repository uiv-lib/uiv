    "Carousel",
    "Slider",
    "Collapse",
    "Dropdown",
    "Nav",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Tooltip",
    "Typeahead",
    "Auto complete",
    "Multi select"
  ]
}
