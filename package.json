{
  "name": "uiv",
  "version": "1.4.0",
  "description": "Bootstrap 3 components implemented by Vue 2.",
  "author": "wxsm",
  "main": "dist/uiv.common.js",
  "module": "dist/uiv.esm.js",
  "unpkg": "dist/uiv.min.js",
  "jsdelivr": "dist/uiv.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiv-lib/uiv.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uiv-lib/uiv/issues"
  },
  "homepage": "https://uiv.wxsm.space",
  "scripts": {
    "dev": "node build/dev-server.js",
    "dist": "npm run dist:esm && npm run dist:esm-single && npm run dist:cjs && npm run dist:umd",
    "dist:umd": "rollup -c build/rollup.umd.js",
    "dist:cjs": "rollup -c build/rollup.cjs.js",
    "dist:esm": "rollup -c build/rollup.esm.js",
    "dist:esm-single": "rollup -c build/rollup.esm-single.js",
    "dist:test": "rollup -c build/rollup.test.js",
    "build": "node build/build-docs.js && node build/build-dist-config.js",
    "test": "jest --coverage",
    "test:watch": "yarn test --watch",
    "lint": "eslint --ext .js,.vue src test build",
    "lint:fix": "yarn lint --fix"
  },
  "dependencies": {
    "portal-vue": "^2.1.7",
    "vue-functional-data-merge": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.8",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "21.0.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.0.6",
    "@rollup/plugin-replace": "3.0.0",
    "@vue/test-utils": "^1.2.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.3.1",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "3.4.1",
    "cross-env": "7.0.3",
    "es6-object-assign": "1.1.0",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-vue": "8.0.3",
    "jest": "^27.3.1",
    "jest-serializer-vue": "^2.0.2",
    "lodash": "4.17.21",
    "prettier": "2.4.1",
    "rollup": "2.59.0",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-strip-code": "0.2.7",
    "rollup-plugin-uglify": "6.0.4",
    "rollup-plugin-vue": "5.1.9",
    "vue": "2.6.14",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "2.6.14"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "test/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*",
      "!src/__test__/**/*",
      "!**/*.snap"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Bootstrap",
    "Bootstrap 3",
    "Bootstrap for Vue",
    "SSR",
    "Web",
    "Components",
    "Directives",
    "Vue",
    "VueJS",
    "Vue2",
    "WebComponents",
    "vue-strap",
    "vuestrap",
    "CSS",
    "Affix",
    "MessageBox",
    "Notification",
    "ScrollSpy",
    "Alert",
    "Button",
    "Button group",
    "Breadcrumbs",
    "Carousel",
    "Slider",
    "Collapse",
    "Dropdown",
    "Nav",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Tooltip",
    "Typeahead",
    "Auto complete",
    "Multi select"
  ]
}
