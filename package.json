{
  "name": "uiv",
  "version": "2.0.0",
  "description": "Bootstrap 3 components implemented by Vue.",
  "author": "wxsm",
  "main": "./dist/uiv.umd.js",
  "module": "./dist/uiv.es.js",
  "exports": {
    ".": {
      "import": "./dist/uiv.es.js",
      "require": "./dist/uiv.umd.js"
    }
  },
  "unpkg": "./dist/uiv.umd.js",
  "jsdelivr": "./dist/uiv.umd.js",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiv-lib/uiv.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uiv-lib/uiv/issues"
  },
  "homepage": "https://uiv.wxsm.space",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run docs:dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "dist": "vite build -c build/vite.config.js && node build/single.js",
    "test": "jest --coverage",
    "test:watch": "pnpm test -- --watch",
    "lint": "eslint --ext .js,.vue src build docs/.vitepress",
    "lint:fix": "pnpm lint -- --fix"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@vitejs/plugin-vue": "2.1.0",
    "@vitejs/plugin-vue-jsx": "1.3.3",
    "@vue/babel-plugin-jsx": "1.1.1",
    "@vue/compiler-sfc": "3.2.30",
    "@vue/test-utils": "2.0.0-rc.18",
    "@vue/vue3-jest": "27.0.0-alpha.4",
    "axios": "0.25.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "27.5.1",
    "bootstrap": "3.4.1",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.4.1",
    "jest": "27.5.1",
    "jest-serializer-vue": "2.0.2",
    "less": "4.1.2",
    "lodash": "4.17.21",
    "prettier": "2.5.1",
    "sinon": "13.0.1",
    "vite": "2.7.13",
    "vitepress": "0.21.6",
    "vue": "3.2.30",
    "vue-i18n": "9.2.0-beta.30",
    "vue-jest": "5.0.0-alpha.10"
  },
  "peerDependencies": {
    "vue": "^3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/__test__/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "test/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*",
      "!src/__test__/**/*",
      "!**/*.snap"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "@vue/vue3-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "snapshotSerializers": [
      "jest-serializer-vue"
    ],
    "testEnvironment": "jsdom"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Bootstrap",
    "Bootstrap 3",
    "Bootstrap for Vue",
    "SSR",
    "Web",
    "Components",
    "Directives",
    "Vue",
    "VueJS",
    "Vue3",
    "WebComponents",
    "vue-strap",
    "vuestrap",
    "CSS",
    "Affix",
    "MessageBox",
    "Notification",
    "ScrollSpy",
    "Alert",
    "Button",
    "Button group",
    "Breadcrumbs",
    "Carousel",
    "Slider",
    "Collapse",
    "Dropdown",
    "Nav",
    "Modal",
    "Navbar",
    "Pagination",
    "Popover",
    "Progress",
    "Tab",
    "Tooltip",
    "Typeahead",
    "Auto complete",
    "Multi select"
  ]
}
