{
  "name": "uiv",
  "version": "1.0.1",
  "description": "Bootstrap 3 components implemented by Vue 2.",
  "author": "wxsm",
  "main": "dist/uiv.common.js",
  "module": "dist/uiv.esm.js",
  "unpkg": "dist/uiv.min.js",
  "jsdelivr": "dist/uiv.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uiv-lib/uiv.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uiv-lib/uiv/issues"
  },
  "homepage": "https://uiv.wxsm.space",
  "scripts": {
    "dev": "node build/dev-server.js",
    "dist": "npm run dist:esm && npm run dist:esm-single && npm run dist:cjs && npm run dist:umd",
    "dist:umd": "rollup -c rollup/rollup.umd.js",
    "dist:cjs": "rollup -c rollup/rollup.cjs.js",
    "dist:esm": "rollup -c rollup/rollup.esm.js",
    "dist:esm-single": "rollup -c rollup/rollup.esm-single.js",
    "dist:test": "rollup -c rollup/rollup.test.js",
    "build": "node build/build-docs.js && node build/build-dist-config.js",
    "test": "cross-env NODE_ENV=test karma start test/karma.conf.js --single-run",
    "lint": "eslint --ext .js,.vue src test/specs",
    "coveralls": "cat test/coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "dependencies": {
    "portal-vue": "^2.1.7",
    "vue-functional-data-merge": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-buble": "0.21.3",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.3.3",
    "bootstrap": "3.4.1",
    "chai": "4.2.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "es6-object-assign": "1.1.0",
    "eslint": "4.19.1",
    "eslint-config-standard": "11.0.0",
    "eslint-plugin-html": "4.0.6",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "jquery": "3.5.1",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-mocha": "2.0.1",
    "karma-rollup-preprocessor": "7.0.5",
    "karma-sinon-chai": "2.0.2",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "lodash": "4.17.20",
    "mocha": "8.1.3",
    "mocha-lcov-reporter": "1.3.0",
    "puppeteer": "5.3.1",
    "rollup": "2.28.2",
    "rollup-plugin-filesize": "9.0.2",
    "rollup-plugin-istanbul": "2.0.1",
    "rollup-plugin-strip-code": "0.2.7",
    "rollup-plugin-uglify": "6.0.4",
    "rollup-plugin-vue": "5.1.9",
    "sinon": "9.2.0",
    "sinon-chai": "3.5.0",
    "vue": "2.6.12",
    "vue-template-compiler": "2.6.12"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "keywords": [
    "Bootstrap",
    "Bootstrap 3",
    "Bootstrap for Vue",
    "SSR",
    "Web",
    "Components",
    "Directives",
    "Vue",
    "VueJS",
    "Vue2",
    "WebComponents",
    "vue-strap",
    "vuestrap",
    "CSS",
    "Affix",
    "MessageBox",
    "Notification",
    "ScrollSpy",
    "Alert",
    "Button",
